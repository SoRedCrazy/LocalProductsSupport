<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="Loginscene" prefHeight="480" prefWidth="720" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scene.LoginControleur">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="173.0" layoutY="153.0" prefHeight="125" prefWidth="375">
         <children>
            <Label text="Mail ou Siret:" />
            <TextField fx:id="mail" />
            <Label layoutX="10.0" layoutY="10.0" text="Mot de passe:" />
            <TextField fx:id="mdp" layoutX="10.0" layoutY="27.0" />
            <Button onAction="#connection" text="Connexion" />
         </children>
      </VBox>
      <Text fx:id="titre" fill="#dd0f0f" layoutX="264.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Se connecter" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="34.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
